package com.nad.ds;

import java.util.Stack;

public class StackProblem {

	public static Stack<Integer> sortStack(Stack<Integer> stk){
		Stack<Integer> tempStack=new Stack<Integer>();
		
		while(!stk.isEmpty()) {
			int val=stk.pop();
			
			while(!tempStack.isEmpty() && tempStack.peek()>val) {
				stk.push(val);
			}
			tempStack.push(val);
		}
		return tempStack;
	}
}
